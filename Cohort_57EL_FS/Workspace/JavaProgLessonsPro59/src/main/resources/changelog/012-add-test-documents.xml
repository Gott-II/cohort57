        xsi:schemaLocation="
                http://www.liquibase.org/xml/ns/dbchangelog
                https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.23.xsd">

    <changeSet id="012-add-test-documents" author="ait" context="test">
        <insert tableName="client_documents_db">
            <column name="client_email" value="test@mail.com"/>
            <column name="doc_type" value="PASSPORT"/>
            <column name="content_type" value="application/pdf"/>
            <column name="size" valueNumeric="12345"/>
